<mapper namespace="edu.lgxy.lbj.asms.mapper.TeacherMapper">

    <insert id="insertContestDeclaration" parameterType="edu.lgxy.lbj.asms.entity.ContestDeclaration">
    insert into contest_declaration
    (declarant,contest_name,contest_type,contest_date,participants_number,contest_introduction,
     contest_rules
     )
    values
    (#{declarant},#{contestName},#{contestType},#{contestDate},#{participantsNumber},#{contestIntroduction},
     #{contestRules} )
    </insert>

